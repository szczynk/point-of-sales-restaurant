    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "json-server": "^0.17.4",
    "json-server-auth": "^2.1.0",
    "postcss": "^8.4.31",
    "prettier-plugin-tailwindcss": "^0.5.6",
