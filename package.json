    "axios": "^1.6.0",
    "daisyui": "^3.9.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-daisyui": "^4.1.2",
    "react-datepicker": "^4.21.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-icons": "^4.11.0",
